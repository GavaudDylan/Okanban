{
  "name": "s12-okanban-api-enzoclock",
  "version": "1.0.0",
  "description": "Le joyeux Kanban des Pavlova",
  "directories": {
    "doc": "docs"
  },
  "type": "module",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "db:create": "psql -U okanban -d okanban -f data/create_tables.sql",
    "db:populate": "psql -U okanban -d okanban -f data/populate_tables.sql",
    "db:reset": "npm run db:create && npm run db:populate"
  },
  "keywords": [
    "kanban",
    "board",
    "gestion de projet"
  ],
  "author": "pavlova@oclock.io",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "pg": "^8.12.0",
    "sequelize": "^6.37.3"
  },
  "devDependencies": {
    "eslint": "^9.5.0",
    "nodemon": "^3.1.4"
  }
}
